---
layout: false
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//hacksw/handcal//NONSGML v1.0//EN
<%- entries.each do |entry| -%>
BEGIN:VEVENT
UID: <%= entry.uid %>
CREATED: <%= entry.created.strftime '%Y%m%dT%H%M%S' %>
DTSTART:<%= entry.start.strftime '%Y%m%dT%H%M%S' %>
DTEND:<%= entry.end.strftime '%Y%m%dT%H%M%S' %>
SUMMARY:Café Vie Privée
LOCATION:<%= entry.location %>
<%- if entry.link -%>
URL:<%= entry.link %>
<%- end -%>
END:VEVENT
<%- end -%>
END:VCALENDAR
